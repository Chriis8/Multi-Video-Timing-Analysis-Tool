<?xml version="1.0" encoding="UTF-8"?>
<interface>
    <object class="GtkApplicationWindow" id="main_window">
        <property name="default-width">1600</property>
        <property name="default-height">1200</property>
        <child>
            <object class="GtkBox" id="container">
                <style><class name="container"></class></style>
                <property name="orientation">vertical</property>
                <property name="spacing">10</property>
                <child>
                    <object class="GtkBox" id="main_box">
                        <style><class name="main-box"></class></style>
                        <property name="orientation">horizontal</property>
                        <child>
                            <object class="GtkScrolledWindow" id="video_scroll_window">
                                <style><class name="video-scroll-window"></class></style>
                                <property name="width-request">1600</property>
                                <property name="hscrollbar_policy">GTK_POLICY_NEVER</property>
                                <property name="vscrollbar_policy">GTK_POLICY_AUTOMATIC</property>
                                <property name="hexpand">true</property>
                                <property name="halign">fill</property>
                                <child>
                                    <object class="GtkFlowBox" id="video_container">
                                        <style><class name="video-container"></class></style>
                                        <property name="vexpand">true</property>
                                        <property name="hexpand">true</property>
                                        <property name="halign">fill</property>
                                        <property name="column-spacing">0</property>
                                        <property name="row-spacing">0</property>                                        
                                    </object>
                                </child>
                            </object>
                        </child>
                        <child>
                            <object class="GtkButton" id="new_video_player_button">
                                <style><class name="new-video-player-button"></class></style>
                                <property name="vexpand">true</property>
                                <property name="label">New Player</property>
                            </object>
                        </child>
                        <!-- <child>
                            <object class="GtkButton" id="add_column_button">
                                <property name="label">Add Column</property>
                            </object>
                        </child> -->

                        <child>
                            <object class="GtkButton" id="add_row_button">
                                <property name="label">Add Row</property>
                            </object>
                        </child>
                        
                        <child>
                            <object class="GtkButton" id="remove_column_button">
                                <property name="label">Remove Column</property>
                            </object>
                        </child>
                        
                        <child>
                            <object class="GtkButton" id="remove_row_button">
                                <property name="label">Remove </property>
                            </object>
                        </child>
                        
                        <child>
                            <object class="GtkScrolledWindow" id="scroll-window">
                                <property name="vexpand">true</property>
                                <property name="width-request">200</property>
                                <property name="hexpand">true</property>
                                <child>
                                    <object class="GtkBox" id="split_container">
                                        <style><class name="split-container"></class></style>
                                        <property name="orientation">vertical</property>
                                        <property name="vexpand">true</property>
                                        <property name="width-request">200</property>
                                    </object>
                                </child>
                            </object>
                        </child>
                    </object>
                </child>
                <child>
                    <object class="GtkBox" id="bottom_container">
                        <style><class name="bottom-container"></class></style>
                        <property name="hexpand">true</property>
                        <property name="height-request">100</property>
                        <child>
                            <object class="GtkButton" id="new_split">
                                <property name="label">new split</property>
                            </object>
                        </child>
                        <child>
                            <object class="GtkButton" id="print_splits_button">
                                <property name="label">print splits</property>
                            </object>
                        </child>
                    </object>
                </child>
            </object>
        </child>
    </object>
</interface>